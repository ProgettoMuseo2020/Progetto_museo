<Window x:Class="Progetto_museo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Progetto_museo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <Image Source="Ingresso_museo_SAME.jpg" Stretch="Fill"/>
        <Button x:Name="Btn_primo" Content="Carica" HorizontalAlignment="Left" Margin="10,114,0,0" VerticalAlignment="Top" Width="75" Click="Btn_primo_Click"/>
        <Button x:Name="Btn_secondo" Content="Carica" HorizontalAlignment="Left" Margin="271,114,0,0" VerticalAlignment="Top" Width="75" Click="Btn_secondo_Click"/>
        <Button x:Name="Btn_terzo" Content="Carica" HorizontalAlignment="Left" Margin="10,288,0,0" VerticalAlignment="Top" Width="75" Click="Btn_terzo_Click"/>
        <Button x:Name="Btn_quarto" Content="Carica" HorizontalAlignment="Left" Margin="271,288,0,0" VerticalAlignment="Top" Width="75" Click="Btn_quarto_Click"/>
        <ListBox x:Name="Lst_primo" HorizontalAlignment="Left" Height="121" Margin="90,15,0,0" VerticalAlignment="Top" Width="160"/>
        <ListBox x:Name="Lst_secondo" HorizontalAlignment="Left" Height="121" Margin="351,15,0,0" VerticalAlignment="Top" Width="156"/>
        <ListBox x:Name="Lst_terzo" HorizontalAlignment="Left" Height="126" Margin="90,184,0,0" VerticalAlignment="Top" Width="160"/>
        <ListBox x:Name="Lst_quarto" HorizontalAlignment="Left" Height="126" Margin="351,184,0,0" VerticalAlignment="Top" Width="156"/>

    </Grid>
</Window>
