<Window x:Class="Progetto_museo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Progetto_museo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <Image Source="Ingresso_museo_SAME.jpg" Stretch="Fill"/>
        <Button x:Name="Btn_primo" Content="Carica" HorizontalAlignment="Left" Margin="10,87,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="Txt_primo" HorizontalAlignment="Left" Height="99" Margin="90,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Txt_primo_TextChanged"/>
        <Button x:Name="Btn_secondo" Content="Carica" HorizontalAlignment="Left" Margin="271,93,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="Txt_secondo" HorizontalAlignment="Left" Height="105" Margin="368,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Txt_primo_TextChanged"/>
        <Button x:Name="Btn_terzo" Content="Carica" HorizontalAlignment="Left" Margin="10,262,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="Txt_terzo" HorizontalAlignment="Left" Height="99" Margin="100,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Txt_primo_TextChanged"/>
        <Button x:Name="Btn_quarto" Content="Carica" HorizontalAlignment="Left" Margin="271,262,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="Txt_quarto" HorizontalAlignment="Left" Height="99" Margin="368,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Txt_primo_TextChanged"/>

    </Grid>
</Window>
